'''

seq = fasta.replace('\n', '').split('>')[1:]

def gc(dna):
    dic = {}
    for n in dna:
        name = n[:13]
        seq = n[13:]
        g_and_c = seq.count('G') + seq.count('C')
        length = len(seq)
        gc = g_and_c/length *100
        dic[name] = gc
    return dic
    
    
results = gc(seq)
maximum = max(results, key=results.get)

print(maximum, '\n', results[maximum])
